<resources xmlns:xliff="http://schemas.android.com/tools">
    <string name="app_name">My Movies</string>

    <!--To be used in MovieDetailActivity-->
    <!--Extra title for the intent-->
    <string name="movie_extra">Movie</string>

    <!--Release date-->
    <string name="release_date">
        Release Date: <xliff:g id="month">%1$s</xliff:g>/<xliff:g id="day">%2$s</xliff:g>/<xliff:g id="year">%3$s</xliff:g>
    </string>

    <!--User rating-->
    <string name="user_rating">
        User Rating: <xliff:g id="rating">%1$.1f</xliff:g>
    </string>

    <string name="content_authority">com.example.android.mymovies</string>

</resources>
